<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="25dp">

    <android.support.design.widget.TabItem
        android:id="@+id/tabItem"
        android:layout_width="384dp"
        android:layout_height="567dp"
        android:text="Event Info"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/editText14"
        android:layout_width="107dp"
        android:layout_height="0dp"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="128dp"
        android:contentDescription="eventWhere"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Where"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editText8"
        android:layout_width="107dp"
        android:layout_height="0dp"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="177dp"
        android:contentDescription="eventAddress"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Address"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editText9"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="225dp"
        android:contentDescription="eventWhen"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="When"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editText10"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="270dp"
        android:contentDescription="eventTime"
        android:ems="10"
        android:inputType="time"
        android:text="Time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editText11"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="315dp"
        android:contentDescription="eventGoing"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Total Going"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editText12"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="360dp"
        android:contentDescription="eventItems"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="What to Bring"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name"
        android:layout_width="215dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="60dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="55dp"
        android:contentDescription="eventName"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        android:textSize="33sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tabItem" />

    <ImageButton
        android:id="@+id/eventGoingButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="40dp"
        android:background="@android:color/transparent"
        android:contentDescription="going"
        android:onClick="onEventGoingClick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_check_green_36dp" />

    <ImageButton
        android:id="@+id/eventInterestedButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/transparent"
        android:contentDescription="interested"
        android:onClick="onEventInterestedClick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eventGoingButton"
        app:srcCompat="@drawable/ic_star_border_yellow_36dp" />

    <TextView
        android:id="@+id/totalGoing"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginEnd="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="315dp"
        android:contentDescription="totalGoing"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.36"
        app:layout_constraintStart_toEndOf="@+id/editText11"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="270dp"
        android:contentDescription="time"
        android:ems="10"
        android:inputType="time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editText10"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/when"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="224dp"
        android:contentDescription="when"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editText9"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="176dp"
        android:contentDescription="address"
        android:ems="10"
        android:inputType="textPostalAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editText8"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/where"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="128dp"
        android:contentDescription="where"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.992"
        app:layout_constraintStart_toEndOf="@+id/editText14"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/toBring"
        android:layout_width="229dp"
        android:layout_height="16dp"
        android:layout_marginEnd="72dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="360dp"
        android:contentDescription="whatToBring"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editText12"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>