<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="25dp">

    <android.support.design.widget.TabItem
        android:id="@+id/tabItem"
        android:layout_width="384dp"
        android:layout_height="567dp"
        android:text="Event Info"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="78dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="310dp"
        android:layout_marginTop="177dp"
        android:contentDescription="eventAddress"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Address"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="225dp"
        android:contentDescription="eventWhen"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="When"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="270dp"
        android:contentDescription="eventTime"
        android:ems="10"
        android:inputType="time"
        android:text="Time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="107dp"
        android:layout_height="44dp"
        android:layout_marginEnd="280dp"
        android:layout_marginTop="315dp"
        android:contentDescription="eventGoing"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Total Going"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="260dp"
        android:layout_marginTop="360dp"
        android:contentDescription="eventItems"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="What to Bring"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="215dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="60dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="55dp"
        android:contentDescription="eventName"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tabItem" />

    <EditText
        android:id="@+id/editText14"
        android:layout_width="67dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="315dp"
        android:layout_marginTop="130dp"
        android:contentDescription="eventWhere"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Where"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="55dp"
        android:contentDescription="going"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/holo_green_light" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="80dp"
        android:contentDescription="interested"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/holo_orange_light" />

    <EditText
        android:id="@+id/totalGoing"
        android:layout_width="46dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="230dp"
        android:layout_marginTop="315dp"
        android:contentDescription="totalGoing"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/time"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="192dp"
        android:layout_marginTop="270dp"
        android:contentDescription="time"
        android:ems="10"
        android:inputType="time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/when"
        android:layout_width="84dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="190dp"
        android:layout_marginTop="225dp"
        android:contentDescription="when"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/address"
        android:layout_width="143dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="160dp"
        android:layout_marginTop="175dp"
        android:contentDescription="address"
        android:ems="10"
        android:inputType="textPostalAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/where"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:layout_marginTop="130dp"
        android:contentDescription="where"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/toBring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginTop="360dp"
        android:contentDescription="whatToBring"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>